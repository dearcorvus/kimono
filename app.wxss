/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 用户弹窗样式 */

.justZhi {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 333;
}

.zhiSuccess {
  width: 70%;
  height: 320rpx;
  background: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 10rpx;
  z-index: 555555555555555;
  padding-bottom: 20rpx;
}

.confirm {
  width: 50%;
  height: 65rpx;
  line-height: 65rpx;
  background: #75c4b5;
  color: #fff;
  text-align: center;
  font-size: 26rpx;
  margin: 10rpx auto;
  border-radius: 10rpx;
}

/* 定义组件 */
.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 120rpx;
    border-top: 1rpx solid #d7d7d7; 
}

.tabbar_nav{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 32rpx;
    height: 100%;
}

.tabbar_icon{
    width: 50rpx;
    height:50rpx;
}

.tabbar_nav:nth-child(3) image{ 
    position:relative;
    top:-40rpx;
    width:120rpx;
    height:120rpx;
    border-radius: 50%;
}
